{
    "name": "Ubuntu Base",
    "version": "0.1",
    "slug": "ubuntu-base",
    "description": "Ubuntu Base Image Add-On",
    "auto_uart": true,
    "url": "https://github.com/tringler/hassio-ubuntu-base",
    "startup": "services",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "ports": {
      "2000/tcp": 2000,
      "2001/tcp": 2001,
      "2002/tcp": 2002,
      "2003/tcp": 2003
    },
    "ports_description": {
      "2001/tcp": "HomeGear UI"
    },
    "map": [
      "config:rw",
      "ssl"
    ],
    "schema": {
      "devices": ["match(\/dev\/tty[A-Z]*[0-9]:rwm)"],
      "ssl": "bool"
    },
    "options": {
      "devices": ["/dev/ttyAMA0:rwm"],
      "ssl": false
  }
}